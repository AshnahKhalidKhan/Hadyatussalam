{
  "manifest_version": 3,
  "name": "Hadyatussalam Chrome Extension",
  "version": "1.0",
  "description": "A simple and elegant to-do list extension.",
  "action": 
  {
    "default_popup": "popup/popup.html",
    "default_icon": "/images/favicon.svg"
  },
  "permissions": ["storage"],
  "chrome_url_overrides": 
  {
    "newtab": "newtab.html"
  }
}